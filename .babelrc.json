{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current"
      }
    }]
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "#app": "./src",
        "#components": "./src/components",
        "#lib": "./src/lib",
        "#config": "./src/config",
        "#store": "./src/redux"
      }
    }]
  ]
}
