{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current"
      }
    }]
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "#app": ".",
        "#components": "./components",
        "#lib": "./lib",
        "#config": "./config",
        "#store": "./redux"
      }
    }]
  ]
}
