{
  "presets": [
    ["@babel/preset-env", {
      "targets": "> 0.25%, not dead"
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    ["module-resolver", {
      "alias": {
        "#components": "./src/components",
        "#config": "./src/config",
        "#store": "./src/store",
        "#app": "./src"
      }
    }]
  ]
}
